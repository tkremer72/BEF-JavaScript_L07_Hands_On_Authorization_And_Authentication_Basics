<body style="float: center">
      

      <h1 style="text-align: center">Users in the database</h1>
<div>
      <table>
      <tr >
            <th>View User</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Username</th>
            <th>Email Address</th>
            <th>Remove User</th>
      </tr>
      {{#each users}}
      <tr>
            <td> <a href='/users/{{UserId}}'>View</a></td>
            <td>{{FirstName}}</td>
            <td>{{LastName}}</td>
            <td>{{Username}}</td>
            <td>{{Email}}</td>
            <td><form  method="POST" action="/users/{{UserId}}">
    <button type="submit">Delete</button>
</form></td>
      </tr>
      {{/each}}
</table>
</div>
</body>